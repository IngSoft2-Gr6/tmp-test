{
  "name": "test-repo",
  "version": "1.2.6",
  "main": "index.js",
  "repository": "git@github.com:IngSoft2-Gr6/tmp-test.git",
  "author": "SGman98 <bastiangarnica@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "release:rename": "git branch -m $(echo $(git rev-parse --abbrev-ref HEAD) | cut -d '/' -f1)/$npm_package_version",
    "release:start": "git flow release start x.x.x",
    "release:finish": "git flow release finish $npm_package_version",
    "release:bump": "yarn version --no-git-tag-version --no-commit-hooks --no-push",
    "release:changelog": "changelog",
    "release": "run() { yarn release:start; yarn release:bump $@; yarn release:rename; }; run ",
    "release:full": "run() { yarn release $@; changelog $@; git add package.json CHANGELOG.md; git commit -m \"New version: $npm_package_version\"; yarn release:finish; }; run"
  },
  "devDependencies": {
    "generate-changelog": "^1.8.0"
  }
}
